    "rimraf": "^6.1.2",
    "rollup": "^4.55.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/sprinkles": "^1.6.5"
  }
