{
  "name": "@idealjs/camphora-styled",
  "version": "0.0.32",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
